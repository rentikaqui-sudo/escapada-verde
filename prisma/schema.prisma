
generator client {
    provider = "prisma-client-js"
    binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
    output = "/home/ubuntu/escapada_verde_landing/app/node_modules/.prisma/client"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model ContactLead {
  id        String   @id @default(cuid())
  name      String
  email     String
  phone     String?
  message   String
  finca     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
